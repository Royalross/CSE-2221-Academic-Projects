/**
 * Model class.
 *
 * @author Bruce W. Weide
 * @author Paolo Bucci
 */
public final class AppendModel1 {

    /**
     * Model variables.
     */
    private String input, output;

    /**
     * Default constructor.
     */
    public AppendModel1() {
        /*
         * Initialize model; both variables start as empty strings
         */
        this.input = "";
        this.output = "";
    }

    public void setInput(String input) {
        this.input = input;
    }

    public String input() {
        return this.input;
    }

    public void setOutput(String output) {
        this.output = output;
    }

    public String output() {
        return this.output;
    }

}
